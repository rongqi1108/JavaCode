<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>servletdemo</servlet-name>
        <servlet-class>com.rong.servletdemo01.ServletDemo01</servlet-class>
        <init-param>
            <param-name>namespace</param-name>
            <param-value>this is a namespace</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>servletdemo</servlet-name>
        <url-pattern>/servlet1</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>servletdemo02</servlet-name>
        <servlet-class>com.rong.servletdemo01.ServletDemo02</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servletdemo02</servlet-name>
        <url-pattern>/servlet2</url-pattern>
    </servlet-mapping>

    <context-param>
        <param-name>context-namespace</param-name>
        <param-value>this is a context-namespace</param-value>
    </context-param>
</web-app>